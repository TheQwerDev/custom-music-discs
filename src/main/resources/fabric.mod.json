{
  "schemaVersion": 1,
  "id": "custommusicdiscs",
  "version": "${version}",
	"icon": "assets/custommusicdiscs/icon.png",

  "name": "Custom Music Discs",
  "description": "This mod allows you to add new songs to play on your jukebox!",
  "authors": [
    "TheQwerDev"
  ],
  "contact": {
    "homepage": "https://github.com/TheQwerDev/custom-music-discs",
    "issues": "https://github.com/TheQwerDev/custom-music-discs/issues"
  },

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "client": [
			"theqwerdev.custommusicdiscs.client.CustomMusicDiscsClient"
    ],
		"server": [
			"theqwerdev.custommusicdiscs.server.CustomMusicDiscsServer"
		],
		"afterClientStart": [
			"theqwerdev.custommusicdiscs.client.CustomMusicDiscsClient"
		],
		"modmenu": [
			"theqwerdev.custommusicdiscs.config.ModMenuModule"
		]
  },
  "mixins": [
    "custommusicdiscs.mixins.json"
  ],
	"accessWidener": "custommusicdiscs.accesswidener",
	"depends": {
    "fabricloader": ">=0.13.3",
		"halplibe": ">=4.1.3"
  },
  "suggests": {
  }
}
